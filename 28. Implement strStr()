O(n^2) time, O(1) space
class Solution(object):
    def strStr(self, haystack, needle):
        """
        :type haystack: str
        :type needle: str
        :rtype: int
        """
        
        if not needle: return 0
        
        size = len(needle)
        for idx in range(len(haystack)-size+1):
            if haystack[idx] == needle[0] and haystack[idx:idx+size] == needle:
                return idx
        return -1
