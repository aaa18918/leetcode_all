class Solution(object):
    def getFactors(self, n):
        """
        :type n: int
        :rtype: List[List[int]]
        """
        
        assert n > 0
        
        res = []
        def myDFS(n, p):
            if n < 2: res.append(p)
            
            for i in range(p[-1], n+1):
                if n%i == 0:
                    myDFS(n/i, p+[i])
        
        for i in range(2, n/2+1):
            if n%i == 0:
                myDFS(n/i, [i])
        
        return res

