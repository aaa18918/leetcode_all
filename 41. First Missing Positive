O(n) time, O(1) space
class Solution(object):
    def firstMissingPositive(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        
        idx, size = 0, len(nums)
        
        while idx < size:
            if nums[idx] != idx+1 and 0 < nums[idx] <= size and nums[nums[idx]-1] != nums[idx]:
                nums[nums[idx]-1], nums[idx] = nums[idx], nums[nums[idx]-1]
            else:
                idx += 1
        
        for idx in range(size):
            if nums[idx] != idx+1:
                return idx+1
        
        return size+1
