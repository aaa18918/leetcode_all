O(n) time, O(1) space
class Solution(object):
    def maxSubArray(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        
        res, cur_sum = -sys.maxint-1, 0
        for num in nums:
            cur_sum += num
            res = max(cur_sum, res)
            cur_sum = max(cur_sum, 0)
        return res


