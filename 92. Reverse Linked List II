O(n) time, O(1) space
# Definition for singly-linked list.
# class ListNode(object):
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution(object):
    def reverseBetween(self, head, m, n):
        """
        :type head: ListNode
        :type m: int
        :type n: int
        :rtype: ListNode
        """
        
        if m >= n:
            return head
        
        dummy_head = ListNode(0)
        dummy_head.next = head
        p = dummy_head
        for _ in range(m-1):
            p = p.next
        
        left = p
        q = p.next
        for _ in range(n-m+1):
            tmp = q.next
            q.next = p
            p, q = q, tmp
        
        left.next.next = q
        left.next = p
        return dummy_head.next
