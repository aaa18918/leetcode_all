O(n) time, O(1) space
class Solution(object):
    def maxProfit(self, prices):
        """
        :type prices: List[int]
        :rtype: int
        """
        
        res, left_min = 0, sys.maxint
        for price in prices:
            left_min = min(left_min, price)
            res = max(res, price-left_min)
        return res
