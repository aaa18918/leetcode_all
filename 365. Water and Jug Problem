O(lgn) time, O(1) space
class Solution(object):
    def canMeasureWater(self, x, y, z):
        """
        :type x: int
        :type y: int
        :type z: int
        :rtype: bool
        """

        if z == 0 or x+y == z: return True
        if x == 0 or y == 0 or x+y < z: return False
        
        def GCD(x, y):
            while y:
                temp = y
                y = x%y
                x = temp
            return x
        
        gcd = GCD(x, y) if x > y else GCD(y, x)
        return z%gcd == 0
