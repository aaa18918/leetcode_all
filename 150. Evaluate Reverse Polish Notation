O(n) time, O(n) space
class Solution(object):
    def evalRPN(self, tokens):
        """
        :type tokens: List[str]
        :rtype: int
        """
        
        my_stack = []
        for t in tokens:
            if t in '+-*/':
                b = my_stack.pop()
                a = my_stack.pop()
                if t == '+':
                    my_stack.append(a+b)
                elif t == '-':
                    my_stack.append(a-b)
                elif t == '*':
                    my_stack.append(a*b)
                elif t == '/':
                    if a/b < 0:
                        my_stack.append(-(abs(a)/abs(b)))
                    else:
                        my_stack.append(a/b)
            else:
                my_stack.append(int(t))
        
        return my_stack[0] if my_stack else 0
