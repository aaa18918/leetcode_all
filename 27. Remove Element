O(n) time, O(1) space
class Solution(object):
    def removeElement(self, nums, val):
        """
        :type nums: List[int]
        :type val: int
        :rtype: int
        """
        
        left, right = 0, len(nums)-1
        
        while left <= right:
            if nums[left] == val:
                nums[right], nums[left] = val, nums[right]
                right -= 1
            else:
                left += 1
        
        return left
