O(n) time, O(n) space
class Solution(object):
    def simplifyPath(self, path):
        """
        :type path: str
        :rtype: str
        """
        
        my_stack = []
        for p in path.split('/'):
            if p == '..':
                if my_stack:
                    my_stack.pop()
            elif p == '.' or not p:
                pass
            else:
                my_stack.append(p)
        
        return '/'+'/'.join(my_stack)
