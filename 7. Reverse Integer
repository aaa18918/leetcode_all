O(32) time, O(32) space
class Solution(object):
    def reverse(self, x):
        """
        :type x: int
        :rtype: int
        """
        
        res = 0
        sign = 1 if x >= 0 else -1
        x = abs(x)
        while x > 0:
            res = res*10+x%10
            x /= 10
        res = sign*res
        return res if -2147483648 <= res <= 2147483647 else 0
