O(n) time, O(1) space
class Solution(object):
    def majorityElement(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        
        res, count = 0, 0
        for num in nums:
            if count == 0:
                count, res = 1, num
            else:
                if num == res:
                    count += 1
                else:
                    count -= 1
        
        return res
