O(n) time, O(n) space
class Solution(object):
    def summaryRanges(self, nums):
        """
        :type nums: List[int]
        :rtype: List[str]
        """
        
        left, size, res = 0, len(nums), []
        for idx in range(size):
            if idx == size-1 or nums[idx]+1 != nums[idx+1]:
                if left == idx:
                    res.append(str(nums[idx]))
                else:
                    res.append("{}->{}".format(nums[left], nums[idx]))
                left = idx+1
        return res
