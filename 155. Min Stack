class MinStack(object):

    def __init__(self):
        """
        initialize your data structure here.
        """
        
        self.stack = []
        self.min_nums = []
        

    def push(self, x):
        """
        :type x: int
        :rtype: void
        """
        
        self.stack.append(x)
        if not self.min_nums:
            self.min_nums.append(x)
        else:
            if x > self.min_nums[-1]:
                self.min_nums.append(self.min_nums[-1])
            else:
                self.min_nums.append(x)
        

    def pop(self):
        """
        :rtype: void
        """
        
        self.stack.pop()
        self.min_nums.pop()

    def top(self):
        """
        :rtype: int
        """
        
        return self.stack[-1]

    def getMin(self):
        """
        :rtype: int
        """
        
        return self.min_nums[-1]


# Your MinStack object will be instantiated and called as such:
# obj = MinStack()
# obj.push(x)
# obj.pop()
# param_3 = obj.top()
# param_4 = obj.getMin()


class MinStack(object):

    def __init__(self):
        """
        initialize your data structure here.
        """
        
        self.stack = []
        self.min_nums = []
        

    def push(self, x):
        """
        :type x: int
        :rtype: void
        """
        
        self.stack.append(x)
        if not self.min_nums or self.min_nums[-1] >= x:
            self.min_nums.append(x)
        

    def pop(self):
        """
        :rtype: void
        """
        
        cur_num = self.stack.pop()
        if cur_num == self.min_nums[-1]:
            self.min_nums.pop()

    def top(self):
        """
        :rtype: int
        """
        
        return self.stack[-1]

    def getMin(self):
        """
        :rtype: int
        """
        
        return self.min_nums[-1]


# Your MinStack object will be instantiated and called as such:
# obj = MinStack()
# obj.push(x)
# obj.pop()
# param_3 = obj.top()
# param_4 = obj.getMin()
