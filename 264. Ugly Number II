O(n) time, O(n) space
class Solution(object):
    def nthUglyNumber(self, n):
        """
        :type n: int
        :rtype: int
        """
        
        dp, i2, i3, i5 = [1 for _ in range(n)], 0, 0, 0
        for i in range(1, n):
            cur2, cur3, cur5 = dp[i2]*2, dp[i3]*3, dp[i5]*5
            cur = min(cur2, cur3, cur5)
            
            if cur == cur2:
                i2 += 1
                
            if cur == cur3:
                i3 += 1
            
            if cur == cur5:
                i5 += 1
            
            dp[i] = cur
        
        return dp[-1]
