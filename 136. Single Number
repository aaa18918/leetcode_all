O(n) time, O(1) space
class Solution(object):
    def singleNumber(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        
        res = nums[0]
        for idx in range(1, len(nums)):
            res ^= nums[idx]
        return res


O(n) time, O(1) space
class Solution(object):
    def singleNumber(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        
        return reduce(lambda x, y: x^y, nums)
