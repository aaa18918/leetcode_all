O(496) time, O(1) space
class Solution(object):
    def divide(self, dividend, divisor):
        """
        :type dividend: int
        :type divisor: int
        :rtype: int
        """
        
        sign = -1 if dividend < 0 < divisor or divisor < 0 < dividend else 1
        m, n = abs(dividend), abs(divisor)
        
        res = 0
        while m >= n:
            tmp = n
            cur = 1
            while tmp <= m:
                cur <<= 1
                tmp <<= 1
            
            m -= tmp >> 1
            res += cur >> 1
        
        return min(2**31-1, max(-2**31, res*sign))
