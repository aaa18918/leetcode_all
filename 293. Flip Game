O(n^2) time, O(n^2) space
class Solution(object):
    def generatePossibleNextMoves(self, s):
        """
        :type s: str
        :rtype: List[str]
        """
        
        res = []
        for idx in range(len(s)-1):
            if s[idx] == '+' and s[idx+1] == '+':
                res.append(s[:idx]+'--'+s[idx+2:])
        
        return res
