O(n) time, O(n) space
class Solution(object):
    def evalRPN(self, tokens):
        """
        :type tokens: List[str]
        :rtype: int
        """
        
        my_stack = []
        for string in tokens:
            if string in '+-*/':
                b = my_stack.pop()
                a = my_stack.pop()
                
                if string == '+':
                    my_stack.append(a+b)
                elif string == '-':
                    my_stack.append(a-b)
                elif string == '*':
                    my_stack.append(a*b)
                else:
                    sign = -1 if a/b < 0 else 1
                    my_stack.append(sign*(abs(a)/abs(b)))
            else:
                my_stack.append(int(string))
        
        return my_stack[0] if tokens else 0
