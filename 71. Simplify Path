O(n) time, O(n) space
class Solution(object):
    def simplifyPath(self, path):
        """
        :type path: str
        :rtype: str
        """
        
        my_stack = []
        for string in path.split('/'):
            if string:
                string = string.strip('/')
                
                if string == '..':
                    if my_stack:
                        my_stack.pop()
                elif string != '.':
                    my_stack.append(string)
        
        return '/'+'/'.join(my_stack)
