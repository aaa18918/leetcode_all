class Solution(object):
    def findDuplicate(self, paths):
        """
        :type paths: List[str]
        :rtype: List[List[str]]
        """
        
        hash_table = {}
        
        for path in paths:
            path_list = path.split(' ')
            directory = path_list[0]
            for file_and_content in path_list[1:]:
                content_start_index = file_and_content.find('(')
                content = file_and_content[content_start_index: -1]
                if content in hash_table:
                    hash_table[content].append(directory+'/'+file_and_content[:content_start_index])
                else:
                    hash_table[content] = [directory+'/'+file_and_content[:content_start_index]]
        
        res = []
        for content, dir_list in hash_table.iteritems():
            if len(dir_list) > 1:
                res.append(dir_list)
        
        return res
