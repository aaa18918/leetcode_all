class Solution(object):
    def findRestaurant(self, list1, list2):
        """
        :type list1: List[str]
        :type list2: List[str]
        :rtype: List[str]
        """
        
        hash_table = {}
        for i, name in enumerate(list1):
            if name not in hash_table:
                hash_table[name] = i
        
        res, least_index_sum = [], sys.maxint
        for i, name in enumerate(list2):
            if name in hash_table:
                if hash_table[name]+i < least_index_sum:
                    res = [name]
                    least_index_sum = hash_table[name]+i
                elif hash_table[name]+i == least_index_sum:
                    res.append(name)
        
        return res
