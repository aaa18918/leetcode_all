O(???) time, O(???) space
class Solution(object):
    def generateParenthesis(self, n):
        """
        :type n: int
        :rtype: List[str]
        """
        
        res = []
        
        def helper(cur, left, right):
            if left == right == 0:
                res.append(cur)
                return
            
            if left < right:
                helper(cur+')', left, right-1)
            
            if left > 0:
                helper(cur+'(', left-1, right)
        
        helper('', n, n)
        return res
