O(n) time, O(1) space
class Solution(object):
    def removeDuplicates(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        
        left = 2
        for idx in range(2, len(nums)):
            if nums[idx] != nums[left-2]:
                nums[left] = nums[idx]
                left += 1
         
        return left if len(nums) >= 2 else len(nums)
