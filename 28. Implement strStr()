O(n^2) time, O(len_needle) space
class Solution(object):
    def strStr(self, haystack, needle):
        """
        :type haystack: str
        :type needle: str
        :rtype: int
        """
        
        if not needle: return 0
        
        len_haystack, len_needle = len(haystack), len(needle)
        for idx in range(len_haystack+1-len_needle):
            if haystack[idx] == needle[0] and haystack[idx:idx+len_needle] == needle:
                return idx
        return -1
