O(mn) time, O(mn) space
class Solution(object):
    def uniquePaths(self, m, n):
        """
        :type m: int
        :type n: int
        :rtype: int
        """
        
        if m < 1 or n < 1:
            return 0
        
        record = [[1]*n for _ in range(m)]
        
        for i in range(m-2, -1, -1):
            for j in range(n-2, -1, -1):
                record[i][j] = record[i+1][j]+record[i][j+1]
        
        return record[0][0]
