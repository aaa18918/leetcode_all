O(lgn) time, O(1) space
class Solution(object):
    def search(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: int
        """
        
        left, right = 0, len(nums)-1
        while left <= right:
            mid = (left+right)/2
            if nums[mid] == target:
                return mid
            
            if nums[0] <= nums[mid] and nums[0] > nums[-1]:
                if target > nums[mid] or target <= nums[-1]:
                    left = mid+1
                else:
                    right = mid-1
            else:
                if nums[-1] < target or target < nums[mid]:
                    right = mid-1
                else:
                    left = mid+1
        
        return -1
