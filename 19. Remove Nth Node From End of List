O(n) time, O(1) space
# Definition for singly-linked list.
# class ListNode(object):
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution(object):
    def removeNthFromEnd(self, head, n):
        """
        :type head: ListNode
        :type n: int
        :rtype: ListNode
        """
        
        dummy_head = ListNode(0)
        dummy_head.next = head
        
        p, q = dummy_head, dummy_head
        for _ in range(n):
            q = q.next
        
        while q.next:
            p, q = p.next, q.next
        
        p.next = p.next.next
        return dummy_head.next
