O(lgn) time, O(1) space
class Solution(object):
    def canAttendMeetings(self, intervals):
        """
        :type intervals: List[List[int]]
        :rtype: bool
        """
        
        intervals.sort()
        
        for idx in range(1, len(intervals)):
            if intervals[idx][0] < intervals[idx-1][1]:
                return False
        
        return True
