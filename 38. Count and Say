O(n^2) time, O(n) space
class Solution(object):
    def countAndSay(self, n):
        """
        :type n: int
        :rtype: str
        """

        cur = '1'
        for i in range(1, n):
            temp, count = '', 1
            for idx in range(1, len(cur)):
                if cur[idx] != cur[idx-1]:
                    temp += str(count)+cur[idx-1]
                    count = 1
                else:
                    count += 1
            
            cur = temp+str(count)+cur[-1]
        
        return cur
