O(81*2) time, O(9) space
class Solution(object):
    def isValidSudoku(self, board):
        """
        :type board: List[List[str]]
        :rtype: bool
        """
        
        def isPartialValid(x1, y1, x2, y2):
            my_set = set()
            for i in range(x1, x2+1):
                for j in range(y1, y2+1):
                    if board[i][j] != '.':
                        if board[i][j] in my_set:
                            return False
                        else:
                            my_set.add(board[i][j])
            
            return True
        
        for idx in range(9):
            if not isPartialValid(idx, 0, idx, 8):
                return False
            
            if not isPartialValid(0, idx, 8, idx):
                return False
        
        for i in range(3):
            for j in range(3):
                if not isPartialValid(3*i, 3*j, 3*i+2, 3*j+2):
                    return False
        
        return True
