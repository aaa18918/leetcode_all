O(n!) time, O(n*n!) space
class Solution(object):
    def permuteUnique(self, nums):
        """
        :type nums: List[int]
        :rtype: List[List[int]]
        """
        
        nums.sort()
        
        return [[nums[idx]]+p for idx in range(len(nums)) for p in self.permuteUnique(nums[:idx]+nums[idx+1:]) if idx == 0 or nums[idx] != nums[idx-1]] if nums else [[]]
