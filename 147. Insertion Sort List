# Definition for singly-linked list.
# class ListNode(object):
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution(object):
    def insertionSortList(self, head):
        """
        :type head: ListNode
        :rtype: ListNode
        """
        
        if not head: return head
        fake_head = ListNode(0)
        fake_head.next = head
        
        p = head    #p.next.val is the key value
        while p.next:
            prev = fake_head
            while prev is not p and prev.next.val <= p.next.val:
                prev = prev.next
            
            if prev is not p:
                temp = p.next
                p.next = temp.next
                temp.next = prev.next
                prev.next = temp
            else:
                p = p.next
        
        return fake_head.next
