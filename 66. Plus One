O(n) time, O(n) space
class Solution(object):
    def plusOne(self, digits):
        """
        :type digits: List[int]
        :rtype: List[int]
        """
        
        cur_sum = 1
        for idx in range(len(digits)-1, -1, -1):
            cur_sum += digits[idx]
            digits[idx] = cur_sum%10
            cur_sum /= 10
            
            if cur_sum == 0:
                break
        
        return digits if cur_sum == 0 else [1]+digits
