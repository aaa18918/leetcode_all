O(n) time, O(1) space
class Solution(object):
    def removeDuplicates(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        
        if len(nums) <= 2:
            return len(nums)
        
        left = 2
        for idx in range(2, len(nums)):
            if nums[idx] != nums[left-2]:
                if idx > left:
                    nums[left] = nums[idx]
                left += 1
        return left
