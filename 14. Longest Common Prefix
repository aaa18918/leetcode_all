O(mn) time, O(m) space
class Solution(object):
    def longestCommonPrefix(self, strs):
        """
        :type strs: List[str]
        :rtype: str
        """
        
        def getCommonPrefix(l, r):
            for idx in range(min(len(l), len(r))):
                if l[idx] != r[idx]:
                    return l[:idx]
            return l if len(l) < len(r) else r
        
        return reduce(getCommonPrefix, strs) if strs else ""
