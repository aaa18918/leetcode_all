O(n) time, O(n) space
class Solution(object):
    def isValid(self, s):
        """
        :type s: str
        :rtype: bool
        """
        
        my_stack = []
        mapping = {')':'(', ']':'[', '}':'{'}
        for char in s:
            if char in '([{':
                my_stack.append(char)
            else:
                if my_stack and mapping[char] == my_stack[-1]:
                    my_stack.pop()
                else:
                    return False
        
        return not my_stack
