O(n) time, O(n) space
class Solution(object):
    def compareVersion(self, version1, version2):
        """
        :type version1: str
        :type version2: str
        :rtype: int
        """
        
        version1, version2 = [int(v) for v in version1.split('.')], [int(v) for v in version2.split('.')]
        len_version1, len_version2 = len(version1), len(version2)
        idx1, idx2 = 0, 0
        while idx1 < len_version1 or idx2 < len_version2:
            if idx1 < len_version1 and idx2 < len_version2:
                if version1[idx1] < version2[idx2]:
                    return -1
                elif version1[idx1] > version2[idx2]:
                    return 1
                else:
                    idx1, idx2 = idx1+1, idx2+1
            elif idx1 < len_version1:
                if version1[idx1] > 0:
                    return 1
                else:
                    idx1 += 1
            elif idx2 < len_version2:
                if version2[idx2] > 0:
                    return -1
                else:
                    idx2 += 1
        return 0
