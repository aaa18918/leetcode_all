O(n^2) time, O(1) space
class Solution(object):
    def triangleNumber(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        
        nums.sort()
        
        res = 0
        for i in range(len(nums)-1, 1, -1):
            j, k = 0, i-1
            while j < k:
                if nums[j]+nums[k] > nums[i]:
                    res += k-j
                    k -= 1
                else:
                    j += 1
        
        return res
