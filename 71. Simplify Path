O(n) time, O(n) space
class Solution(object):
    def simplifyPath(self, path):
        """
        :type path: str
        :rtype: str
        """
        
        path = path.split('/')
        my_stack = []
        for p in path:
            if not p or p == '.':
                pass
            elif p == "..":
                if my_stack:
                    my_stack.pop()
            else:
                my_stack.append(p)
        return '/'+'/'.join(my_stack)
