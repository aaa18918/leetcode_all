O(n) time, O(1) space
class Solution(object):
    def singleNumber(self, nums):
        """
        :type nums: List[int]
        :rtype: List[int]
        """
        
        tmp = reduce(lambda x, y: x^y, nums)
        
        mask = tmp&(-tmp)
        
        a, b = 0, 0
        for num in nums:
            if num&mask:
                a ^= num
            else:
                b ^= num
        
        return [a, b]
