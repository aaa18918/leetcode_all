O(n) time, O(n) space
class Solution(object):
    def canPermutePalindrome(self, s):
        """
        :type s: str
        :rtype: bool
        """
        
        chars_count = collections.Counter(s)
        odd_count = 0
        for count in chars_count.itervalues():
            if count%2 == 1:
                odd_count += 1
        
        return odd_count <= 1
