O(n) time, O(n) space
class Solution(object):
    def numDecodings(self, s):
        """
        :type s: str
        :rtype: int
        """
        
        if not s or s[0] == '0': return 0
        
        dp = [0 for _ in range(len(s)+1)]
        dp[0], dp[1] = 1, 1
        for idx in range(1, len(s)):
            if s[idx] == '0':
                if s[idx-1] not in "12":
                    return 0
                else:
                    dp[idx+1] = dp[idx-1]
            else:
                if s[idx-1] == '1':
                    dp[idx+1] = dp[idx]+dp[idx-1]
                elif s[idx-1] == '2':
                    if '1' <= s[idx] <= '6':
                        dp[idx+1] = dp[idx]+dp[idx-1]
                    else:
                        dp[idx+1] = dp[idx]
                else:
                    dp[idx+1] = dp[idx]
        return dp[len(s)]
