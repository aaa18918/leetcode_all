class Solution(object):
    def generatePossibleNextMoves(self, s):
        """
        :type s: str
        :rtype: List[str]
        """
        
        if len(s) < 2: return []
        
        res = []
        for index, char in enumerate(s[:-1]):
            if char == s[index+1] == '+':
                res.append(s[:index]+'-'*2+s[index+2:])
        return res
