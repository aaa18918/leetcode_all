O(n) time, O(1) space
class Solution(object):
    def maxProfit(self, prices):
        """
        :type prices: List[int]
        :rtype: int
        """
        
        res, min_value = 0, sys.maxint
        for price in prices:
            res = max(res, price-min_value)
            min_value = min(min_value, price)
        return res
