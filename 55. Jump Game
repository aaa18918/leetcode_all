O(n) time, O(1) space
class Solution(object):
    def canJump(self, nums):
        """
        :type nums: List[int]
        :rtype: bool
        """
        
        cur = len(nums)-1
        for idx in range(len(nums)-2, -1, -1):
            if nums[idx]+idx >= cur:
                cur = idx
        return cur == 0
