O(???) time, O(???) space
class Solution(object):
    def findStrobogrammatic(self, n):
        """
        :type n: int
        :rtype: List[str]
        """
        
        res, nums = [], '01689'
        mappings = {'1':'1', '6':'9', '9':'6', '8':'8', '0':'0'}
        
        def myDFS(cur):
            if len(cur) == n:
                if n == 1 or cur[0] != '0':
                    res.append(cur)
                return
            
            for num in nums:
                myDFS(num+cur+mappings[num])
        
        if n%2 == 0:
            myDFS('')
        else:
            for num in '018':
                myDFS(num)
        
        return res
