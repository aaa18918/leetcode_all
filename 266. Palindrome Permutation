O(n) time, O(n) space
class Solution(object):
    def canPermutePalindrome(self, s):
        """
        :type s: str
        :rtype: bool
        """
        
        counter = collections.Counter(s)
        count = 0
        for val in counter.itervalues():
            if val%2 != 0:
                count += 1
                
                if count > 1:
                    return False
        
        return True
