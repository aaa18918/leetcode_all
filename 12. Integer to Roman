O(13) time, O(13) space
class Solution(object):
    def intToRoman(self, num):
        """
        :type num: int
        :rtype: str
        """
        
        values = [1000, 900, 500, 400, 100, 90, 50, 40, 10, 9, 5, 4, 1]
        strings = ["M", "CM", "D", "CD", "C", "XC", "L", "XL", "X", "IX", "V", "IV", "I"]
        
        res = ""
        for idx in range(len(values)):
            while num >= values[idx]:
                res += strings[idx]
                num -= values[idx]
        
        return res
