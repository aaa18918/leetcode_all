O(n) time, O(1) space
class Solution(object):
    def compareVersion(self, version1, version2):
        """
        :type version1: str
        :type version2: str
        :rtype: int
        """
        
        version1, version2 = version1.split('.'), version2.split('.')
        size1, size2 = len(version1), len(version2)
        i, j = 0, 0
        while i < size1 or j < size2:
            if i < size1 and j < size2:
                if int(version1[i]) < int(version2[j]):
                    return -1
                elif int(version1[i]) > int(version2[j]):
                    return 1
                else:
                    i += 1
                    j += 1
            elif i < size1:
                if int(version1[i]) > 0:
                    return 1
                else:
                    i += 1
            elif j < size2:
                if int(version2[j]) > 0:
                    return -1
                else:
                    j += 1
        return 0
