O(n) time, O(1) space
class Solution(object):
    def rob(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        
        if not nums:
            return 0
        elif len(nums) == 1:
            return nums[0]
        
        a, b = nums[0], nums[0]
        for idx in range(2, len(nums)-1):
            a, b = b, max(b, a+nums[idx])
        
        res = b
        
        a, b = 0, nums[1]
        for idx in range(2, len(nums)):
            a, b = b, max(b, a+nums[idx])
        
        res = max(res, b)
        return res
