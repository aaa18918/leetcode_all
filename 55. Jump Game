O(n) time, O(1) space
class Solution(object):
    def canJump(self, nums):
        """
        :type nums: List[int]
        :rtype: bool
        """
        
        max_position = 0
        size = len(nums)
        for idx in range(size):
            if idx > max_position:
                return False
            
            max_position = max(max_position, idx+nums[idx])
            
            if max_position >= size:
                return True
        
        return True
