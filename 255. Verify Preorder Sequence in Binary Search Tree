O(n) time, O(n) space
class Solution(object):
    def verifyPreorder(self, preorder):
        """
        :type preorder: List[int]
        :rtype: bool
        """
        
        my_stack, low_boundry = [], float('-inf')
        for num in preorder:
            if num < low_boundry:
                return False
            
            while my_stack and num > my_stack[-1]:
                low_boundry = max(low_boundry, my_stack.pop())
            
            my_stack.append(num)
        
        return True
