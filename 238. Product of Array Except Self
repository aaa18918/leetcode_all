O(n) time, O(1) extra space excepts the output array
class Solution(object):
    def productExceptSelf(self, nums):
        """
        :type nums: List[int]
        :rtype: List[int]
        """
        
        res = [1]
        cur = 1
        for idx in range(len(nums)-1):
            cur *= nums[idx]
            res.append(cur)
        
        cur = 1
        for idx in range(len(nums)-1, 0, -1):
            cur *= nums[idx]
            res[idx-1] *= cur
        
        return res if nums else []
