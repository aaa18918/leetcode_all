O(n) time, O(n) space
class Solution(object):
    def canPermutePalindrome(self, s):
        """
        :type s: str
        :rtype: bool
        """
        
        counter = collections.Counter(s)
        odd_chars_num = 0
        for count in counter.values():
            if count%2 == 1:
                odd_chars_num += 1
        return odd_chars_num <= 1
