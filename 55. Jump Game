O(n) time, O(1) space
class Solution(object):
    def canJump(self, nums):
        """
        :type nums: List[int]
        :rtype: bool
        """
        
        max_distance = 0
        for idx in range(len(nums)):
            if idx > max_distance:
                return False
            
            max_distance = max(max_distance, idx+nums[idx])
        return True
