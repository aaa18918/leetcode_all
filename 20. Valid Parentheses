O(n) time, O(n) space
class Solution(object):
    def isValid(self, s):
        """
        :type s: str
        :rtype: bool
        """
        
        mapping = {')':'(', ']':'[', '}':'{'}
        my_stack = []
        
        for char in s:
            if char in '([{':
                my_stack.append(char)
            else:
                if not my_stack or my_stack[-1] != mapping[char]:
                    return False
                else:
                    my_stack.pop()
        
        return not my_stack
