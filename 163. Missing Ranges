O(n) time, O(1) space
class Solution(object):
    def findMissingRanges(self, nums, lower, upper):
        """
        :type nums: List[int]
        :type lower: int
        :type upper: int
        :rtype: List[str]
        """
        
        nums.append(upper+1)
        left = lower-1
        res = []
        for num in nums:
            if num == left+2:
                res.append(str(left+1))
            elif num > left+2:
                res.append('{}->{}'.format(left+1, num-1))
            
            left = num
        return res
