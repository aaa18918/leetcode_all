class Solution(object):
    def findMissingRanges(self, nums, lower, upper):
        """
        :type nums: List[int]
        :type lower: int
        :type upper: int
        :rtype: List[str]
        """
        
        if not nums: return ["{}->{}".format(lower, upper)] if lower < upper else [str(lower)]
        
        assert lower <= nums[0] and upper >= nums[-1]
        nums.append(upper+1)
        
        left, res = lower, []
        for num in nums:
            if num <= left:
                left = max(left, num+1)
                continue
            if num-left == 1:
                res.append(str(left))
            else:
                res.append("{}->{}".format(left, num-1))
            left = num+1
        return res
