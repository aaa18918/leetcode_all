O(???) time, O(???) space
class Solution(object):
    def generateParenthesis(self, n):
        """
        :type n: int
        :rtype: List[str]
        """
        
        def myHelp(left, right):
            res = []
            
            if 0 <= left < right:
                res += [')' + p for p in myHelp(left, right-1)]
            
            if 0 < left:
                res += ['(' + p for p in myHelp(left-1, right)]
            
            return res if res else ['']
        
        return myHelp(n, n) if n > 0 else []
