O(lgn) time hasNext(), O(1) time next(), O(lgn) space
# Definition for a  binary tree node
# class TreeNode(object):
#     def __init__(self, x):
#         self.val = x
#         self.left = None
#         self.right = None

class BSTIterator(object):
    def __init__(self, root):
        """
        :type root: TreeNode
        """
        
        self.p = root
        self.my_stack = []

    def hasNext(self):
        """
        :rtype: bool
        """
        
        while self.p:
            self.my_stack.append(self.p)
            self.p = self.p.left
        
        return not not self.my_stack

    def next(self):
        """
        :rtype: int
        """
        
        top = self.my_stack.pop()
        self.p = top.right
        return top.val

# Your BSTIterator will be called like this:
# i, v = BSTIterator(root), []
# while i.hasNext(): v.append(i.next())
