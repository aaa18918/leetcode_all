O(n) time, O(1) space
class Solution(object):
    def singleNumber(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        
        ones, twos = 0, 0
        
        for number in nums:
            ones = ones ^ number & ~twos
            twos = twos ^ number & ~ones
        
        return ones
