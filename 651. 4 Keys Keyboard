O(n) time, O(n) space
class Solution(object):
    def maxA(self, N):
        """
        :type N: int
        :rtype: int
        """
        
        dp = [i for i in range(N+1)]
        
        for i in range(7, N+1):
            for j in range(2, i-2):
                dp[i] = max(dp[j]*(i-j-1), dp[i])
        return dp[N]
