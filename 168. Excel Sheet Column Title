O(lgn) time, O(1) space
class Solution(object):
    def convertToTitle(self, n):
        """
        :type n: int
        :rtype: str
        """
        
        res, n = "", n-1
        while n >= 0:
            res = chr(n%26+ord('A'))+res
            n = n/26-1
        return res


O(n) time, O(1) space
class Solution(object):
    def convertToTitle(self, n):
        """
        :type n: int
        :rtype: str
        """
        
        res = ''
        while n > 0:
            r = n%26
            if r == 0:
                res += 'Z'
                n = n/26-1
            else:
                res += chr(ord('A')-1+r)
                n /= 26
        
        return res[::-1]
