class Solution(object):
    def findRestaurant(self, list1, list2):
        """
        :type list1: List[str]
        :type list2: List[str]
        :rtype: List[str]
        """
        
        hash_table = {}
        for i, name in enumerate(list1):
            if name not in hash_table:
                hash_table[name] = i
        
        res, least_index_sum = [], sys.maxint
        for i, name in enumerate(list2):
            if name in hash_table:
                if hash_table[name]+i < least_index_sum:
                    res = [name]
                    least_index_sum = hash_table[name]+i
                elif hash_table[name]+i == least_index_sum:
                    res.append(name)
        
        return res


class Solution(object):
    def findRestaurant(self, list1, list2):
        """
        :type list1: List[str]
        :type list2: List[str]
        :rtype: List[str]
        """
        
        hash_table = collections.defaultdict(int)
        for idx, r in enumerate(list1):
            hash_table[r] = idx
        
        res, min_idx_sum = [], sys.maxint
        for idx, r in enumerate(list2):
            cur_idx_sum = idx+hash_table.get(r, sys.maxint)
            if cur_idx_sum < min_idx_sum:
                res, min_idx_sum = [r], cur_idx_sum
            elif cur_idx_sum == min_idx_sum:
                res.append(r)
        return res
