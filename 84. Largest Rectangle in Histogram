O(n) time, O(n) space
class Solution(object):
    def largestRectangleArea(self, heights):
        """
        :type heights: List[int]
        :rtype: int
        """
        
        my_stack, res = [-1], 0
        heights.append(0)
        for idx in range(len(heights)):
            while heights[idx] < heights[my_stack[-1]]:
                h = heights[my_stack.pop()]
                res = max(res, (idx-my_stack[-1]-1)*h)
            
            my_stack.append(idx)
        
        heights.pop()
        return res
