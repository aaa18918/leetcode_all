O(constant) time, O(3^4*n) space
class Solution(object):
    def restoreIpAddresses(self, s):
        """
        :type s: str
        :rtype: List[str]
        """
        
        res = []
        
        for a in range(1, 4):
            for b in range(1, 4):
                for c in range(1, 4):
                    for d in range(1, 4):
                        if a+b+c+d == len(s):
                            nums = [s[:a], s[a:a+b], s[a+b:a+b+c], s[a+b+c:]]
                            
                            if all(map(lambda x: int(x) <= 255 and (len(x) == 1 or x[0] != '0'), nums)):
                                res.append(s[:a]+'.'+s[a:a+b]+'.'+s[a+b:a+b+c]+'.'+s[a+b+c:])

        return res


O(???) time, O(???) space
class Solution(object):
    def restoreIpAddresses(self, s):
        """
        :type s: str
        :rtype: List[str]
        """
        
        res, size = [], len(s)
        
        def myDFS(idx, cur):
            for i in range(idx, min(idx+3, size)):
                num = int(s[idx:i+1])
                if 0 <= num <= 255:
                    if i+1-idx > 1 and s[idx] == '0':
                        return
                        
                    if len(cur) == 3:
                        if i+1 != size:
                            continue
                        else:
                            res.append('.'.join(cur+[s[idx:i+1]]))
                    else:
                        myDFS(i+1, cur+[s[idx:i+1]])
        
        myDFS(0, [])
        
        return res
