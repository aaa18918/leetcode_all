https://leetcode.com/problems/palindrome-number/

70%:
class Solution(object):
    def isPalindrome(self, x):
        """
        :type x: int
        :rtype: bool
        """
        
        if x < 0: return False
        copy = x
        reverse = 0
        while copy:
            reverse = reverse*10+copy%10
            copy /= 10
        
        return reverse == x

80%:
class Solution(object):
    def isPalindrome(self, x):
        """
        :type x: int
        :rtype: bool
        """
        
        if x < 0: return False
        x = str(x)
        
        return x == x[::-1]

86%:
class Solution(object):
    def isPalindrome(self, x):
        """
        :type x: int
        :rtype: bool
        """
        
        if x < 0: return False
        x = str(x)
        len_x = len(x)
        for i in xrange(len_x/2):
            if x[i] != x[len_x-1-i]: return False
        return True

89%:
class Solution(object):
    def isPalindrome(self, x):
        """
        :type x: int
        :rtype: bool
        """
        
        if x < 0 or x > 0 and x%10 == 0: return False
        cur_sum = 0
        while x > cur_sum:
            cur_sum = 10*cur_sum+x%10
            x /= 10
        return x == cur_sum or x == cur_sum/10
