O(32) time, O(32) space
class Solution(object):
    def reverse(self, x):
        """
        :type x: int
        :rtype: int
        """
        
        m = abs(x)
        res = 0
        while m > 0:
            res = res*10+m%10
            m /= 10
        
        if x < 0:
            res = -res
        
        return res if -2**31 <= res < 2**31 else 0
