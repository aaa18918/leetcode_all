O(lgn) time, O(1) space
class Solution(object):
    def search(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: int
        """
        
        left, right = 0, len(nums)-1
        while left <= right:
            mid = (left+right)/2
            if nums[mid] == target:
                return mid
            elif nums[mid] < target:
                if nums[mid] <= nums[-1] < target:
                    right = mid-1
                else:
                    left = mid+1
            else:
                if target <= nums[-1] < nums[mid]:
                    left = mid+1
                else:
                    right = mid-1
        
        return -1
