O(n) time, O(1) space
# Definition for singly-linked list.
# class ListNode(object):
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution(object):
    def swapPairs(self, head):
        """
        :type head: ListNode
        :rtype: ListNode
        """
        
        dummy_head = ListNode(0)
        h = dummy_head
        p = head
        
        while p:
            q = p.next
            if not q:
                h.next = p
                h = h.next
                break
            else:
                temp = p.next.next
                h.next = q
                q.next = p
                h = h.next.next
                p = temp
        
        h.next = None
        return dummy_head.next
