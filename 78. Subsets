O(2^n) time, O(2^n) space
class Solution(object):
    def subsets(self, nums):
        """
        :type nums: List[int]
        :rtype: List[List[int]]
        """
        
        if not nums:
            return [[]]
        
        num = nums.pop()
        prev = self.subsets(nums)
        new = []
        for p in prev:
           new.append(p+[num])

        return prev+new
