O(496) time, O(1) space
class Solution(object):
    def divide(self, dividend, divisor):
        """
        :type dividend: int
        :type divisor: int
        :rtype: int
        """
        
        m, n = abs(dividend), abs(divisor)
        
        res = 0
        while m >= n:
            tmp = n
            cur = 1
            while m >= tmp:
                tmp <<= 1
                cur <<= 1
            
            m -= tmp>>1
            res += cur>>1
        
        if dividend < 0 and divisor > 0 or dividend > 0 and divisor < 0:
            res = -res
        
        return max(-2**31, min(2**31-1, res))
