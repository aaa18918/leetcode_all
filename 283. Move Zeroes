O(n) time, O(1) space
class Solution(object):
    def moveZeroes(self, nums):
        """
        :type nums: List[int]
        :rtype: void Do not return anything, modify nums in-place instead.
        """
        
        left = 0
        for idx in range(len(nums)):
            if nums[idx] != 0:
                if left != idx:
                    nums[left], nums[idx] = nums[idx], nums[left]
                
                left += 1
