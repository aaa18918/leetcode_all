O(n) time, O(1) space
# Definition for singly-linked list.
# class ListNode(object):
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution(object):
    def deleteDuplicates(self, head):
        """
        :type head: ListNode
        :rtype: ListNode
        """
        
        p, q = head, head
        while q:
            if q.val != p.val:
                p.next = q
                p = p.next
            
            q = q.next
        
        if p:
            p.next = None
        
        return head
