O(n^2) time, O(n) space
class Solution(object):
    def countPrimes(self, n):
        """
        :type n: int
        :rtype: int
        """
        
        prime = [True]*n
        count = 0
        for idx in range(2, n):
            if prime[idx]:
                count += 1
                for j in range(idx*idx, n, idx):
                    prime[j] = False
        return count
