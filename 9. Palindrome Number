O(32) time, O(32) space
class Solution(object):
    def isPalindrome(self, x):
        """
        :type x: int
        :rtype: bool
        """
        
        if x == 0: return True
        if x%10 == 0: return False
        
        temp = 0
        while x > 0:
            if x == temp or x/10 == temp:
                return True
            temp = temp*10+x%10
            x /= 10
        return False
