O(n) time, O(1) space
class Solution(object):
    def maxProfit(self, prices):
        """
        :type prices: List[int]
        :rtype: int
        """
        
        prev_min, res = sys.maxint, 0
        for price in prices:
            prev_min = min(prev_min, price)
            res = max(res, price-prev_min)
        
        return res
