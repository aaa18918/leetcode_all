O(n) time, O(n) space
class Solution(object):
    def exclusiveTime(self, n, logs):
        """
        :type n: int
        :type logs: List[str]
        :rtype: List[int]
        """
        
        my_stack = []
        res = [0 for _ in range(n)]
        for log in logs:
            f_id, s_or_e, t = log.split(':')
            f_id, t = int(f_id), int(t)
            if s_or_e == 'start':
                if my_stack:
                    res[my_stack[-1][0]] += t-my_stack[-1][1]
                
                my_stack.append([f_id, t])
            else:
                res[f_id] += t-my_stack[-1][1]+1
                my_stack.pop()
                
                if my_stack:
                    my_stack[-1][1] = t+1
        
        return res
