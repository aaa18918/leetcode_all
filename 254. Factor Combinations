class Solution(object):
    def getFactors(self, n):
        """
        :type n: int
        :rtype: List[List[int]]
        """
        
        if n < 2:
            return []
        
        res = []
        
        def myDFS(start, n, c):
            if n == 1:
                if len(c) > 1:
                    res.append(c)
                
                return
            
            for i in range(start, n+1):
                if n%i == 0:
                    myDFS(i, n/i, c+[i])
        
        myDFS(2, n, [])
        return res


