O(n) time, O(n) space
class Solution(object):
    def isIsomorphic(self, s, t):
        """
        :type s: str
        :type t: str
        :rtype: bool
        """
        
        if len(s) != len(t):
            return False
        
        mapping1, mapping2 = {}, {}
        for idx in range(len(s)):
            if s[idx] in mapping1:
                if mapping1[s[idx]] != t[idx]:
                    return False
            else:
                mapping1[s[idx]] = t[idx]
            
            if t[idx] in mapping2:
                if mapping2[t[idx]] != s[idx]:
                    return False
            else:
                mapping2[t[idx]] = s[idx]
        
        return True
