O(n) time, O(1) space
class Solution(object):
    def maxProfit(self, prices):
        """
        :type prices: List[int]
        :rtype: int
        """
        
        if not prices:
            return 0
        
        left_min, res = prices[0], 0
        for idx in range(len(prices)):
            res = max(res, prices[idx]-left_min)
            left_min = min(left_min, prices[idx])
        
        return res
