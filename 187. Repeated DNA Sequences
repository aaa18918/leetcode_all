O(n) time, O(n) space
class Solution(object):
    def findRepeatedDnaSequences(self, s):
        """
        :type s: str
        :rtype: List[str]
        """
        
        res = set()
        my_set = set()
        
        for idx in range(len(s)-9):
            string = s[idx:idx+10]
            if string in my_set:
                res.add(string)
            else:
                my_set.add(string)
        
        return [r for r in res]
