O(n) time, O(1) space
# Definition for singly-linked list.
# class ListNode(object):
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution(object):
    def rotateRight(self, head, k):
        """
        :type head: ListNode
        :type k: int
        :rtype: ListNode
        """
        
        if not head:
            return None
        
        p = head
        count = 1
        while p.next:
            p = p.next
            count += 1
        
        k %= count
        p.next = head
        for _ in range(count-k):
            p = p.next
        
        res = p.next
        p.next = None
        return res
