O(???) time, O(???) space
class Solution(object):
    def wordBreak(self, s, wordDict):
        """
        :type s: str
        :type wordDict: List[str]
        :rtype: List[str]
        """
        
        wordSet, size = set(wordDict), len(s)
        memo = {size: ['']}
        
        def DFS(idx):
            if idx not in memo:
                memo[idx] = [s[idx:i]+(r and ' '+r) for i in range(idx+1, size+1) if s[idx:i] in wordSet for r in DFS(i)]
            
            return memo[idx]
        
        return DFS(0)
