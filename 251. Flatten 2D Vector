O(n) time, O(1) space
class Vector2D(object):

    def __init__(self, vec2d):
        """
        Initialize your data structure here.
        :type vec2d: List[List[int]]
        """
        
        self.vec2d = vec2d
        self.i, self.j = 0, -1

    def next(self):
        """
        :rtype: int
        """
        
        return self.vec2d[self.i][self.j]

    def hasNext(self):
        """
        :rtype: bool
        """
        
        if not self.vec2d: return False
        
        m = len(self.vec2d)
        self.j += 1
        while self.i < m and self.j == len(self.vec2d[self.i]):
            self.i, self.j = self.i+1, 0
        
        if self.i == m:
            return False
        
        return True

# Your Vector2D object will be instantiated and called as such:
# i, v = Vector2D(vec2d), []
# while i.hasNext(): v.append(i.next())
