O(n) time, O(lgn) space
# Definition for a binary tree node.
# class TreeNode(object):
#     def __init__(self, x):
#         self.val = x
#         self.left = None
#         self.right = None

class Solution(object):
    def isValidBST(self, root):
        """
        :type root: TreeNode
        :rtype: bool
        """
        
        prev, p, my_stack = None, root, []
        while p or my_stack:
            while p:
                my_stack.append(p)
                p = p.left
            
            p = my_stack.pop()
            if prev and prev.val >= p.val: return False
            prev = p
            p = p.right
        return True
