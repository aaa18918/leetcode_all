O(n) time, O(1) space
class Solution(object):
    def maxProfit(self, prices):
        """
        :type prices: List[int]
        :rtype: int
        """
        
        res = 0
        for idx in range(1, len(prices)):
            if prices[idx] > prices[idx-1]:
                res += prices[idx]-prices[idx-1]
        return res
